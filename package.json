{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint ./src/**/*.{ts,tsx} App.tsx --no-error-on-unmatched-pattern",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|react-router-native)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@react-native-async-storage/async-storage": "^1.14.1",
    "@react-native-picker/picker": "1.9.2",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "expo": "^40.0.0",
    "expo-status-bar": "~1.0.3",
    "formik": "^2.2.6",
    "graphql": "^14.7.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-paper": "^4.7.2",
    "react-native-web": "~0.13.12",
    "react-router-native": "^5.2.0",
    "use-debounce": "^6.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@expo/webpack-config": "~0.12.45",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.20",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-router-native": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-preset-expo": "8.3.0",
    "eslint": "^7.20.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-native-globals": "^0.1.2",
    "jest": "^26.6.3",
    "jest-expo": "^40.0.0",
    "typescript": "~4.0.0"
  },
  "private": true
}
